<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="800.0" style="-fx-background-color: #d1d1cd;" stylesheets="@../../css/admin.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Admin.UpdateAccountController">
   <children>
      <AnchorPane layoutX="25.0" layoutY="25.0" prefHeight="500.0" prefWidth="750.0" style="-fx-background-color: white; -fx-background-radius: 20px;">
         <children>
            <VBox alignment="CENTER" prefHeight="500.0" prefWidth="750.0">
               <children>
                  <Label prefHeight="42.0" prefWidth="194.0" text="Accounts Managment" textFill="#838383">
                     <effect>
                        <Glow level="0.45" />
                     </effect>
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <Label text="Update Account" textFill="#489ef4">
                     <font>
                        <Font name="System Bold" size="46.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </Label>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox alignment="CENTER" prefWidth="350.0">
                           <children>
                              <Label prefWidth="200.0" text="Username">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="usernameField" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="25.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefWidth="350.0">
                           <children>
                              <Label prefWidth="200.0" text="Account Number">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="accountNumField" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="25.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefWidth="350.0">
                           <children>
                              <Label prefWidth="200.0" text="Balance">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="balanceField" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="25.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox alignment="CENTER" prefWidth="350.0">
                           <children>
                              <Label prefWidth="200.0" text="Currencey">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="currenceyField" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="25.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="updateBtn" mnemonicParsing="false" onAction="#updateBtnHandel" style="-fx-padding: 12; -fx-font-weight: bold;" styleClass="saveButton" text="Update">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="backBtn" mnemonicParsing="false" onAction="#backBtnHandel" style="-fx-padding: 12; -fx-font-weight: bold;" styleClass="cancelButton" text="Back" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
